<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"     
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:vm="using:CTDialog.ViewModels"
        xmlns:views="clr-namespace:CTDialog.Views"
		x:Class="CTDialog.Views.DialogWindowView"   
        mc:Ignorable="d" 
		d:DesignWidth="800" 
		d:DesignHeight="450"     
		TransparencyLevelHint="AcrylicBlur"
		ExtendClientAreaToDecorationsHint="True"
        Title="CTDialog">
	<Panel>
		<ExperimentalAcrylicBorder IsHitTestVisible="False">
			<ExperimentalAcrylicBorder.Material>
				<ExperimentalAcrylicMaterial
                    BackgroundSource="Digger"
                    TintColor="Black"
                    TintOpacity="1"
                    MaterialOpacity="0.65" />
			</ExperimentalAcrylicBorder.Material>
		</ExperimentalAcrylicBorder>
		<Panel Margin="40">
			<WebView Name="WV_Tools" Grid.Column="0" Url="https://forum.valuepickr.com/t/investing-basics-feel-free-to-ask-the-most-basic-questions/1282/1966"/>
		</Panel>
	</Panel>
</Window>
